# Create the queries that will be used to extract data for Solr
INSERT_UPDATE SolrIndexerQuery ; solrIndexedType(identifier)[unique=true] ; identifier[unique=true]            ; type(code) ; injectCurrentDate[default=true] ; injectCurrentTime[default=true] ; injectLastIndexTime[default=true] ; query ; user(uid)
                               ; $solrIndexedType                         ; $searchIndexNamePrefix-fullQuery   ; full       ;                                 ;                                 ; false                             ; "                                 
SELECT distinct{price.pk} FROM {PRICEROW AS price join Product AS p on {price:product} = {p:pk}
              join ProductAvailabilityAssignment AS paa on {paa.product}={p.pk}
              join ProductAvailabilityGroup AS pag on {paa.availabilityGroup} ={pag.pk}
              join ProductAvailabilityGroup2BaseStoreRel AS pag2store on {pag.pk}={pag2store.source}
              join BaseStore AS store on {store.pk}={pag2store.target}
              JOIN ArticleApprovalStatus as appstatus ON {appstatus.pk} = {p.approvalStatus} JOIN  Catalog as c on {c.activeCatalogVersion}={p.catalogVersion}
              }
             where {appstatus.code} = 'approved' 
                             and {price.minAdvertisedPrice} != 1
                             AND ((to_char({price:starttime},'yyyy-MM-dd') <= to_char(NOW(),'yyyy-MM-dd')  or {price:starttime} is null)
                                           and (to_char({price:endtime},'yyyy-MM-dd') >= to_char(NOW(),'yyyy-MM-dd') or {price:endtime} is null)) and {c.id} = 'gpUSProductCatalog'
"                          ; admin                                    
                               ; $solrIndexedType                         ; $searchIndexNamePrefix-updateQuery ; update     ;                                 ;                                 ;                                   ; "                                 
SELECT distinct{price.pk} FROM {PRICEROW AS price join Product AS p on {price:product} = {p:pk} 
	join ProductAvailabilityAssignment AS paa on {paa.product}={p.pk} 
	join ProductAvailabilityGroup AS pag on {paa.availabilityGroup} ={pag.pk} 
	join ProductAvailabilityGroup2BaseStoreRel AS pag2store on {pag.pk}={pag2store.source} 
	join BaseStore AS store on {store.pk}={pag2store.target} 
	JOIN ArticleApprovalStatus as appstatus ON {appstatus.pk} = {p.approvalStatus} JOIN  Catalog as c on {c.activeCatalogVersion}={p.catalogVersion}}
 	where {appstatus.code} = 'approved' 
 		and {price.minAdvertisedPrice} != 1
		AND ({p:code} IN (           
      	 {{                           
         	   SELECT DISTINCT {sl:productCode} FROM {StockLevel AS sl} WHERE {sl:modifiedtime} >= ?lastIndexTime
       	}})                    
		or {price:modifiedtime} >= ?lastIndexTime)
		AND ((to_char({price:starttime},'yyyy-MM-dd') <= to_char(NOW(),'yyyy-MM-dd')  or {price:starttime} is null) 
			and (to_char({price:endtime},'yyyy-MM-dd') >= to_char(NOW(),'yyyy-MM-dd') or {price:endtime} is null)) and {c.id} = 'gpUSProductCatalog'
"                          ; admin                                    
                               ; $solrIndexedType                         ; $searchIndexNamePrefix-deleteQuery ; delete     ;                                 ;                                 ; false                             ; "     
SELECT {price.pk} FROM {PRICEROW AS price join Product AS p on {price:product} = {p:pk} 
	join ProductAvailabilityAssignment AS paa on {paa.product}={p.pk} 
	join ProductAvailabilityGroup AS pag on {paa.availabilityGroup} ={pag.pk} 
	join ProductAvailabilityGroup2BaseStoreRel AS pag2store on {pag.pk}={pag2store.source} 
	join BaseStore AS store on {store.pk}={pag2store.target} 
	JOIN ArticleApprovalStatus as appstatus ON {appstatus.pk} = {p.approvalStatus}}
 	where {appstatus.code} != 'approved' OR {price.minAdvertisedPrice} != 0 OR (to_char({price:starttime},'yyyy-MM-dd') > to_char(NOW(),'yyyy-MM-dd')) 
		or (to_char({price:endtime},'yyyy-MM-dd') < to_char(NOW(),'yyyy-MM-dd'))
"                          ; admin                                    

                               
